#!/usr/bin/env python

import sys, os
import matplotlib
matplotlib.use('qt5agg')
from PyQt5 import QtWidgets, QtGui, QtCore
from pymeteo.UI.acars import MainWindow

def main():
    app = QtWidgets.QApplication(sys.argv)
    w = MainWindow()
    sys.exit(app.exec_())

if __name__ == '__main__':
    main()